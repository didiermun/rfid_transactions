{
	"name": "sveltry",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"start": "svelte-kit start",
		"format": "prettier --write ./src/**/*.{svelte,ts} ./*.{ts,cjs}",
		"lint": "eslint ./src/**/*.{ts,svelte} --ignore-path .gitignore .",
		"lint:fix": "eslint --fix ./src/**/*.{ts,svelte} --ignore-path .gitignore ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "4.21.0",
		"@typescript-eslint/parser": "4.21.0",
		"autoprefixer": "10.2.5",
		"cssnano": "5.0.2",
		"eslint": "7.23.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-svelte3": "3.1.2",
		"postcss": "8.2.15",
		"postcss-load-config": "3.0.1",
		"postcss-nested": "5.0.5",
		"prettier": "2.3.0",
		"prettier-plugin-svelte": "2.2.0",
		"svelte": "3.37.0",
		"svelte-preprocess": "4.7.0",
		"tailwindcss": "2.1.2",
		"tslib": "2.2.0",
		"typescript": "4.2.4",
		"vite": "2.3.2"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.0",
		"axios": "0.21.1"
	}
}
